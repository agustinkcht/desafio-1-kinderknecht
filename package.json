{
  "name": "desafios-kinderknecht",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "start": "node ./server.js",
    "dev": "node ./server.js --env dev",
    "memory": "nodemon ./server.js --env dev --persistence memory",
    "fs": "nodemon ./server.js --env dev --persistence fs",
    "argv": "nodemon ./server.js --env prod --persistence mongo",
    "tdd": "node ./test/sum.tdd.js",
    "fkrp": "node ./src/mocks/products.mock.js --env dev",
    "fkru": "node ./src/mocks/users.mock.js --env dev",
    "artSimplex": "artillery quick --count 20 --num 100 'http://localhost:8080/artillery/simplex' -o ./test/artillery/simplex.json",
    "artComplex": "artillery quick --count 20 --num 100 'http://localhost:8080/artillery/complex' -o ./test/artillery/complex.json",
    "artFlow": "artillery run ./test/artillery/flow.yaml -o ./test/artillery/flow.json",
    "artFlowReport": "artillery report ./test/artillery/flow.json",
    "image": "docker build -t technode-prod .",
    "container": "docker run -p 8080:8080 technode8080",
    "publish": "docker tag technode-prod agustinkcht/technode:1.0.0 & docker push agustinkcht/technode:1.0.0",
    "orq": "kubectl apply -f ./orq.yaml",
    "technode": "minikube service technode-service",
    "mocha": "mocha ./test/mocha --env dev",
    "chai": "mocha ./test/chai --env dev",
    "sup": "mocha ./test/supertest --env dev"
  },
  "keywords": [],
  "author": "Agustin Kinderknecht <agustinkcht@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "artillery-plugin-metrics-by-endpoint": "^1.11.0",
    "bcrypt": "^5.1.1",
    "commander": "^12.1.0",
    "connect-mongo": "^5.1.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "express-compression": "^1.0.2",
    "express-handlebars": "^7.1.2",
    "express-session": "^1.18.0",
    "joi": "^17.13.3",
    "joi-oid": "^1.1.2",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.3.3",
    "mongoose-paginate-v2": "^1.8.0",
    "morgan": "^1.10.0",
    "nodemailer": "^6.9.14",
    "passport": "^0.7.0",
    "passport-google-oauth2": "^0.2.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "session-file-store": "^1.5.0",
    "socket.io": "^4.7.5",
    "supertest": "^7.0.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.13.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.4.1",
    "chai": "^5.1.1",
    "mocha": "^10.7.3",
    "nodemon": "^3.1.0"
  }
}
